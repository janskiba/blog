{
  "name": "blog-workspace",
  "private": true,
  "scripts": {
    "docker:dev": "docker compose --env-file .env -f docker-compose.dev.yml up --build",
    "docker:dev:detached": "docker compose --env-file .env -f docker-compose.dev.yml up -d --build",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "docker:prod": "docker compose --env-file .env -f docker-compose.prod.yml up -d --build",
    "docker:prod:down": "docker compose -f docker-compose.prod.yml down",
    "docker:prod:logs": "docker compose -f docker-compose.prod.yml logs -f"
  }
}
